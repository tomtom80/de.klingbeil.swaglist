{
  "extends": "airbnb",
  "plugins": ["react", "import", "jsx-a11y"],
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },

  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "react/jsx-uses-react": "error",
    "react/jsx-uses-vars": "error",
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/prop-types": 0
  }
}
